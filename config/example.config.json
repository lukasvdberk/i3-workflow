{
  "group-configs": [
    {
      "name": "Firefox and vscode",
      "application_spawners": [
        {
          "amount": 1,
          "application": {
            "execution_path": "/usr/bin/firefox localhost",
            "window_names": [
              "Firefox",
              "firefox"
            ]
          },
          "workspace": {
            "name": "1:1"
          }
        },
        {
          "amount": 4,
          "application": {
            "execution_path": "/snap/bin/code",
            "window_names": [
              "code",
              "VSCode"
            ]
          },
          "workspace": {
            "name": "Coding"
          }
        }
      ]
    },
    {
        "name": "Test terminal group",
        "application_spawners": [
          {
            "amount": 4,
            "application": {
              "execution_path": "/usr/bin/kitty",
              "window_names": [
                "Kitty",
                "kitty"
              ]
            },
            "workspace": {
              "name": "Terminal"
            }
          }
        ]
    }
  ],
  "all_workspaces": [
      {
        "name": "Web"
      },
      {
        "name": "Coding"
      },
      {
        "name": "Terminal"
      }
  ]
}
